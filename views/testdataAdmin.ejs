<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test Data</title>
    <link href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css" rel="stylesheet">
    <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous"> -->
    <link rel="stylesheet" href="assets/css/main.css">
    <script src = "assets/js/script.js"></script>
    <link rel="icon" type="image/x-icon" href="assets/images/lg.jpg"> 
    <style>
     
    </style>
</head>
<body>
    <nav class = "navbar bg-white">
        <div class="container">
            <div class = "navbar-content">
                <div class = "brand-and-toggler">
                    <a href = "index.html" class = "navbar-brand">
                      <img src = "assets/images/lg.jpg" alt = "" class = "navbar-brand-icon h-10 w-14" >
                      <span class = "navbar-brand-text">OffCampus<span>.com</span>
                    </a>
                    <button type = "button" onclick="navMenu()" class = "">
                       <a href="allaccess.html" class="button">Go Back</a>
                    </button>
            
                </div>
            </div>
        </div>
    </nav>

    <br><br>

    <center> <h1 class="mt-5 text-blue-500 text-3xl font-bold ">All responses of students</h1></center><br><br>
  

  <% testdata.forEach((student,index) => { %>
  <section class="text-gray-600 body-font">
        
    <div class="container px-5 py-24 mx-auto" >
        <h3 style="font-weight: bold;">(<%= ++index %>) Full Name: <%= student.FullName %></h3>
    <br>
    <h3 style="font-weight: bold;">Email-ID:<%= student.email %></h3>
    <br><br>
      <div class="flex flex-wrap -m-4">
        <div class="lg:w-1/3 lg:mb-0 mb-6 p-4">
          <div class="h-full text-center">
           
            <p class="leading-relaxed"><%= student.answer1 %></p>
            <span class="inline-block h-1 w-10 rounded bg-blue-500 mt-6 mb-4"></span>
          </div>
        </div>
        <div class="lg:w-1/3 lg:mb-0 mb-6 p-4">
          <div class="h-full text-center">
           
            <p class="leading-relaxed"><%= student.answer2 %></p>
            <span class="inline-block h-1 w-10 rounded bg-blue-500 mt-6 mb-4"></span>
            
          </div>
        </div>
        <div class="lg:w-1/3 lg:mb-0 p-4">
          <div class="h-full text-center">

            <p class="leading-relaxed"><%= student.answer3 %></p>
            <span class="inline-block h-1 w-10 rounded bg-blue-500 mt-6 mb-4"></span>
          
          </div>
        </div>
<br><br>
        <div class="lg:w-1/3 lg:mb-0 mb-6 p-4">
            <div class="h-full text-center">
             
              <p class="leading-relaxed"><%= student.answer4 %></p>
              <span class="inline-block h-1 w-10 rounded bg-blue-500 mt-6 mb-4"></span>
              
            </div>
          </div>

          <div class="lg:w-1/3 lg:mb-0 mb-6 p-4">
            <div class="h-full text-center">
            
              <p class="leading-relaxed"><%= student.answer5 %></p>
              <span class="inline-block h-1 w-10 rounded bg-blue-500 mt-6 mb-4"></span>
              
            </div>
          </div>
      </div>
    </div>
    <hr>
    <br><br>
  </section>
  <% }) %>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>

</body>
</html>
