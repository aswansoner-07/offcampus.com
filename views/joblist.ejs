   <center> <h1 class="mt-5 text-blue-500 text-3xl font-bold ">All Available Jobs for freshers/experience</h1></center><br><br>
  <table class="table table-striped mt-5 ml-4" style="">
    <thead>
      <tr>
        <th>S_no.</th>
        <th>Company_name</th>
        <th>Profile</th>
        <th>YearOfExp.</th>
        <th>YearOfPassing</th>
        <th>Job Description</th>
        <th>CTC</th>
        <th>Apply now</th>

        <!-- Add more columns as necessary -->
      </tr>
    </thead>
    <tbody>
      <% x.forEach((company,index) => { %>
        <tr>
          <td><%= ++index %></td>
          <td><%= company.company_name %></td>
          <td><%= company.jobprofile %></td>
          <td><%= company.yoe %></td>
          <td><%= company.yop %></td>
          <td><%= company.jd %></td>
          <td><%= company.ctc %></td>
          <td><button type="button" onclick="" class="btn btn-primary"><a href="http://localhost:5005/applyform.html">Apply</a></button>
          </td>

          <!-- Add more columns as necessary -->
        </tr>
      <% }) %>
    </tbody>
  </table>

